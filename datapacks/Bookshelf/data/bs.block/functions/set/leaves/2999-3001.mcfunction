execute if score @s bs.block.id matches 2999 run setblock ~ ~ ~ redstone_wire[east=side,north=side,power=12,south=side,west=up]
execute if score @s bs.block.id matches 3000 run setblock ~ ~ ~ redstone_wire[east=side,north=side,power=12,south=side,west=side]
execute if score @s bs.block.id matches 3001 run setblock ~ ~ ~ redstone_wire[east=side,north=side,power=12,south=side,west=none]
